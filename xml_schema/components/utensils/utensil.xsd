<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

	<xs:annotation>
		<xs:appinfo>Utensil</xs:appinfo>
		<xs:documentation xml:lang="en">
			This schema defines the utensil element. It includes the component element
			defined in ./../base/component.xsd. As the utensil element is simple, the extension of the component element is enought to define all their properties.
			Author: Jacobo Tapia
		</xs:documentation>
	</xs:annotation>


	<xs:include schemaLocation="./../base/component.xsd"/>
	
	<xs:complexType name="utensil">
		<xs:complexContent>
			<xs:extension base="component" />
		</xs:complexContent>
   	</xs:complexType>

</xs:schema>